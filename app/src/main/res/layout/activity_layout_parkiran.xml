<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".layout_parkiran"
    android:background="@color/black">

    <ImageView
        android:layout_width="150dp"
        android:layout_height="75dp"
        android:background="@drawable/custom4"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="150dp"
        android:id="@+id/lp_1"/>
    <ImageView
        android:layout_width="150dp"
        android:layout_height="75dp"
        android:background="@drawable/custom4"
        android:layout_below="@+id/lp_1"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="30dp"
        android:id="@+id/lp_2"/>
    <ImageView
        android:layout_width="150dp"
        android:layout_height="75dp"
        android:background="@drawable/custom4"
        android:layout_below="@+id/lp_2"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="30dp"
        android:id="@+id/lp_3"/>
    <ImageView
        android:layout_width="150dp"
        android:layout_height="75dp"
        android:background="@drawable/custom4"
        android:layout_below="@+id/lp_3"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="30dp"
        android:id="@+id/lp_4"/>
    <ImageView
        android:layout_width="150dp"
        android:layout_height="75dp"
        android:background="@drawable/custom4"
        android:layout_below="@+id/lp_4"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="30dp"
        android:id="@+id/lp_5"/>
    <ImageView
        android:layout_width="150dp"
        android:layout_height="75dp"
        android:background="@drawable/custom4"
        android:layout_toRightOf="@+id/lp_1"
        android:layout_marginLeft="90dp"
        android:layout_marginTop="150dp"
        android:id="@+id/lp_6"/>
    <ImageView
        android:layout_width="150dp"
        android:layout_height="75dp"
        android:background="@drawable/custom4"
        android:layout_below="@id/lp_6"
        android:layout_marginLeft="250dp"
        android:layout_marginTop="30dp"
        android:id="@+id/lp_7"/>
    <ImageView
        android:layout_width="150dp"
        android:layout_height="75dp"
        android:background="@drawable/custom4"
        android:layout_below="@id/lp_7"
        android:layout_marginLeft="250dp"
        android:layout_marginTop="30dp"
        android:id="@+id/lp_8"/>
    <ImageView
        android:layout_width="150dp"
        android:layout_height="75dp"
        android:background="@drawable/custom4"
        android:layout_below="@id/lp_7"
        android:layout_marginLeft="250dp"
        android:layout_marginTop="30dp"
        android:id="@+id/lp_8"/>
    <ImageView
        android:layout_width="150dp"
        android:layout_height="75dp"
        android:background="@drawable/custom4"
        android:layout_below="@id/lp_8"
        android:layout_marginLeft="250dp"
        android:layout_marginTop="30dp"
        android:id="@+id/lp_9"/>
    <ImageView
        android:layout_width="150dp"
        android:layout_height="75dp"
        android:background="@drawable/custom4"
        android:layout_below="@id/lp_9"
        android:layout_marginLeft="250dp"
        android:layout_marginTop="30dp"
        android:id="@+id/lp_10"/>






</RelativeLayout>