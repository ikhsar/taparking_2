<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DashboardActivity"
    android:background="@color/white">


    
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="115dp"
        android:background="@drawable/custom2"
        android:id="@+id/lyd1"/>

        <ImageView
            android:layout_width="100dp"
            android:layout_height="50dp"
            android:src="@drawable/logo"
            android:layout_marginLeft="10dp"
            android:id="@+id/d_logo"/>
        
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/d_logo"
            android:text="Loading ..."
            android:textSize="20dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="5dp"
            android:fontFamily="@font/poppinssemibold"
            android:id="@+id/d_txt1"
            android:textColor="@color/white"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Where are you going today?"
                android:layout_below="@id/d_txt1"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="5dp"
                android:fontFamily="@font/poppinsmediumitalic"
                android:textSize="12sp"
                android:textColor="@color/white"/>

        <ImageView
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:background="@drawable/notif"
            android:layout_marginLeft="350dp"
            android:layout_marginTop="40dp"
            android:id="@+id/d_g1"/>

            <RelativeLayout
                android:layout_width="344dp"
                android:layout_height="73dp"
                android:background="@drawable/custom5"
                android:layout_marginTop="150dp"
                android:layout_marginLeft="30dp">

                <RelativeLayout
                    android:layout_width="150dp"
                    android:layout_height="40dp"
                    android:background="@drawable/custom1"
                    android:layout_marginTop="5dp"
                    android:layout_marginLeft="20dp"
                    android:id="@+id/rl_d1">
                    
                </RelativeLayout>
                
                <ImageView
                    android:layout_width="18dp"
                    android:layout_height="18dp"
                    android:src="@drawable/dompet"
                    android:layout_marginLeft="30dp"
                    android:layout_marginTop="15dp"
                    android:id="@+id/d_dompet"/>
                
                <TextView
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@id/d_dompet"
                    android:text="Rp.150.000"
                    android:layout_marginTop="15dp"
                    android:layout_marginLeft="10dp"
                    android:id="@+id/d_txt2"/>
                    
                <ImageView
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:src="@drawable/pay"
                    android:layout_toRightOf="@id/d_txt2"
                    android:layout_marginTop="10dp"
                    android:layout_marginLeft="70dp"/>

                <ImageView
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:src="@drawable/topup"
                    android:layout_toRightOf="@id/d_txt2"
                    android:layout_marginTop="10dp"
                    android:layout_marginLeft="70dp"
                    android:id="@+id/d_topup"/>
                <ImageView
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:src="@drawable/pay"
                    android:layout_toRightOf="@+id/d_topup"
                    android:layout_marginTop="10dp"
                    android:layout_marginLeft="40dp"
                    android:id="@+id/d_pay"/>
                
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Top Up"
                    android:layout_toRightOf="@id/d_txt2"
                    android:fontFamily="@font/poppinssemibold"
                    android:textSize="12sp"
                    android:textColor="@color/black"
                    android:layout_marginTop="36dp"
                    android:layout_marginLeft="62dp"
                    android:id="@+id/d_txt3"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Pay"
                    android:layout_toRightOf="@id/d_txt3"
                    android:fontFamily="@font/poppinssemibold"
                    android:textSize="12sp"
                    android:textColor="@color/black"
                    android:layout_marginTop="36dp"
                    android:layout_marginLeft="29dp"/>

            </RelativeLayout>

    
    <ImageView
        android:layout_width="303dp"
        android:layout_height="139dp"
        android:src="@drawable/custom5"
        android:layout_marginTop="250dp"
        android:layout_marginLeft="50dp"/>
    <ImageView
        android:layout_width="37dp"
        android:layout_height="43dp"
        android:src="@drawable/location"
        android:layout_marginLeft="60dp"
        android:layout_marginTop="270dp"
        android:id="@+id/d_location"/>
    <ImageView
        android:layout_width="37dp"
        android:layout_height="43dp"
        android:layout_toRightOf="@id/d_location"
        android:src="@drawable/booking"
        android:layout_marginLeft="85dp"
        android:layout_marginTop="270dp"
        android:id="@+id/d_booking"/>
    <ImageView
        android:layout_width="37dp"
        android:layout_height="43dp"
        android:layout_toRightOf="@id/d_booking"
        android:src="@drawable/history"
        android:layout_marginLeft="85dp"
        android:layout_marginTop="270dp"
        android:id="@+id/d_history"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Parking"
        android:layout_below="@id/d_location"
        android:layout_marginLeft="55dp"
        android:layout_marginTop="10dp"
        android:textColor="@color/black"
        android:fontFamily="@font/poppinssemibold"
        android:textSize="12sp"
        android:id="@+id/txt_location"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Booking"
        android:layout_below="@id/d_booking"
        android:layout_marginLeft="175dp"
        android:layout_marginTop="10dp"
        android:textColor="@color/black"
        android:fontFamily="@font/poppinssemibold"
        android:textSize="12sp"
        android:id="@+id/txt_booking"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="History"
        android:layout_below="@id/d_history"
        android:layout_marginLeft="300dp"
        android:textColor="@color/black"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/poppinssemibold"
        android:textSize="12sp"
        android:id="@+id/txt_history"/>

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@drawable/custom2"
        android:layout_alignParentBottom="true"
        android:id="@+id/d_bottom"/>

    <ImageView
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_marginLeft="10dp"
        android:src="@drawable/rumah"
        android:layout_alignParentBottom="true"
        android:id="@+id/d_rumah"/>

    <ImageView
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:src="@drawable/mobil"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@id/d_rumah"
        android:layout_marginLeft="40dp"
        android:id="@+id/d_mobil"/>

    <ImageView
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:src="@drawable/qrcode"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@id/d_mobil"
        android:layout_marginLeft="40dp"
        android:id="@+id/d_qrcode"/>

    <ImageView
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:src="@drawable/wallett"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@id/d_qrcode"
        android:layout_marginLeft="40dp"
        android:id="@+id/d_wallet"/>

    <ImageView
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:src="@drawable/gear"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@id/d_wallet"
        android:layout_marginLeft="40dp"
        android:id="@+id/d_setting"/>
















</RelativeLayout>